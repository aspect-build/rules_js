{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        ":dependencyDashboard",
        ":enablePreCommit",
        ":semanticPrefixFixDepsChoreOthers",
        "group:monorepos",
        "group:recommended",
        "replacements:all",
        "workarounds:all"
    ],
    "packageRules": [
        {
            "matchFileNames": ["MODULE.bazel"],
            "enabled": false
        }
    ],
    "labels": ["deps"],
    "dependencyDashboard": true,
    "ignorePaths": [
        "**/node_modules/**",
        "e2e/**",
        "examples/**",
        "js/private/test/**",
        "npm/private/test/**"
    ]
}
